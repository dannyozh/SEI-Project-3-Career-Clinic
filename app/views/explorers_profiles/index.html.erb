<p id="notice"><%= notice %></p>

<h1>Explorers Profiles</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @explorers_profiles.each do |explorers_profile| %>
      <tr>
        <td><%= link_to 'Show', explorers_profile %></td>
        <td><%= link_to 'Edit', edit_explorers_profile_path(explorers_profile) %></td>
        <td><%= link_to 'Destroy', explorers_profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Explorers Profile', new_explorers_profile_path %>
